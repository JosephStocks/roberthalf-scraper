# Robert Half Credentials
ROBERTHALF_USERNAME=your.email@example.com
ROBERTHALF_PASSWORD=your_password_here

# Proxy Toggle
USE_PROXY=true            # Set to true/false to enable/disable proxy

# IProyal Proxy Configuration
IPROYAL_PROXY_SERVER=geo.iproyal.com:12321
IPROYAL_PROXY_AUTH=your_username:your_password_country-us_streaming-1

# Proxy Configuration (uses IProyal by default)
PROXY_SERVER=$IPROYAL_PROXY_SERVER
PROXY_AUTH=$IPROYAL_PROXY_AUTH
PROXY_BYPASS=*.iproyal.com

# Optional Proxy Configuration
MAX_REQUESTS_PER_IP=5      # Maximum requests before IP rotation
MAX_TIME_PER_IP=30        # Maximum seconds before IP rotation
REQUEST_DELAY=2           # Delay between requests in seconds

# Browser Configuration (Optional)
VIEWPORT_WIDTH=1920       # Browser viewport width
VIEWPORT_HEIGHT=1080      # Browser viewport height
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/108.0.0.0 Safari/537.36 